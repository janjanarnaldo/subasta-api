{
  "source": {
    "include": [
      "./config",
      "./api",
    	"README.md"
    ],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|assets/)"
  },
	"plugins": [
	  "plugins/markdown"
	],
  "opts": {
    "template": "./node_modules/docdash",
    "encoding": "utf8",
    "destination": "./assets",
    "recurse": true,
    "verbose": true
  },
  "docdash": {
      "static": true,
      "sort": true
  }
}